{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "provide": {
        "ext-ldap": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit-selenium": ">=1.2",
        "facebook/webdriver": "dev-master",
        "phpunit/phpunit": "4.7.*",
        "sensio/generator-bundle": "2.5.*"
    },
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "2.5.*",
        "doctrine/doctrine-bundle": "1.5.*",
        "twig/extensions": "1.2.*",
        "symfony/assetic-bundle": "2.6.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.7.*",
        "sensio/distribution-bundle": "4.0.*",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.5.*",

        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "kriswallsmith/buzz": "v0.15",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "toooni/fpdf": "dev-master",
        "arsgeografica/signing": "1.1.*",
        "phayes/geophp": "1.2",
        "viscreation/vis-ui.js": "0.0.13",
        "igosuki/compass-mixins": "0.12.*",
        "fontfacekit/open-sans": "1.4.*",
        "eslider/sasscb":  "dev-master",
        "gedmo/doctrine-extensions": "~2.3",
        "imag/ldap-bundle":  "2.3.*",
        "mapbender/mapbender-icons": "dev-master",
        "components/underscore": "1.*",
        "components/joii": "*",

        "mapbender/about": "1.x",
        "mapbender/mobile": "*",
        "mapbender/drupalintegration": "*",
        "mapbender/imageexport": "*",
        "mapbender/ol3-map": "*",
        "mapbender/digitizer": "1.x"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/components",
        "component-baseurl": "/components"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "ATTENTION": "Symlinks creating under MS Windows works only if composer runs with admin permissions",
        "symfony-assets-install": "symlink"
    },
    "repositories": [
        { "type": "git", "url": "https://github.com/mapbender/mapbender-about.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-mobile.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-drupalIntegration.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-imageexport.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-map-ol3.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-digitizer.git"},
        {
            "type": "package",
            "package": {
                "name": "ohvitorino/openlayers3",
                "type": "component",
                "version": "3.5.0",
                "dist": {
                    "url": "http://github.com/ohvitorino/openlayers3/archive/3.5.0.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": ["ol.js"],
                        "styles": ["ol.css"]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/placeholdersjs",
                "type": "component",
                "version": "4.0.1",
                "dist": {
                    "url": "http://github.com/jamesallardice/Placeholders.js/archive/v4.0.1.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "dist/placeholders.min.js",
                            "dist/placeholders.jquery.min.js"
                        ]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/jquerydialogextendjs",
                "type": "component",
                "version": "2.0.4",
                "dist": {
                    "url": "http://github.com/ROMB/jquery-dialogextend/archive/2.0.4.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": ["build/*.min.js"]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "igosuki/compass-mixins",
                "type": "component",
                "version": "0.12.6",
                "dist": {
                    "url": "http://github.com/Igosuki/compass-mixins/archive/v0.12.6.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fontfacekit/open-sans",
                "type": "component",
                "version": "1.4.0",
                "dist": {
                    "url": "http://github.com/FontFaceKit/open-sans/archive/1.4.0.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "styles": [
                            "*.css"
                        ],
                        "files": [
                            "fonts/*/*"
                        ]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "eslider/sasscb",
                "version":"dev-master",
                "source": {
                    "url": "https://github.com/eSlider/sassc-binaries.git",
                    "type": "git",
                    "reference":"develop"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/joii",
                "type": "component",
                "version": "3.1.1",
                "source": {
                    "url": "https://github.com/haroldiedema/joii.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": ["dist/joii.min.js"],
                        "files": ["dist/joii.js"]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
