{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "provide": {
        "ext-ldap": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "0.6.x",
        "phpunit/phpunit-selenium": "1.4.x",
        "facebook/webdriver": "1.0.x",
        "phpunit/phpunit": "4.8.x",
        "sensio/generator-bundle": "2.5.x"
    },
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.7.x",
        "doctrine/orm": "2.5.x",
        "doctrine/doctrine-bundle": "1.5.x",
        "doctrine/doctrine-cache-bundle": "1.0.x",
        "twig/extensions": "1.2.x",
        "symfony/assetic-bundle": "2.6.x",
        "symfony/swiftmailer-bundle": "2.3.x",
        "symfony/monolog-bundle": "2.7.x",
        "sensio/distribution-bundle": "4.0.x",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/security-extra-bundle": "1.5.x",
        "jms/di-extra-bundle": "1.5.x",

        "friendsofsymfony/jsrouting-bundle": "1.5.x",
        "kriswallsmith/buzz": "v0.15",
        "doctrine/doctrine-fixtures-bundle": "2.2.x",
        "toooni/fpdf": "dev-master",
        "arsgeografica/signing": "1.1.x",
        "phayes/geophp": "1.2",
        "viscreation/vis-ui.js": "0.0.x",
        "igosuki/compass-mixins": "0.12.x",
        "fontfacekit/open-sans": "1.4.x",
        "eslider/sasscb":  "dev-master",
        "gedmo/doctrine-extensions": "~2.3",
        "imag/ldap-bundle":  "2.3.x",
        "mapbender/mapbender-icons": "1.3",
        "components/underscore": "1.x",
        "components/joii": "*",

        "mapbender/about": "1.x",
        "mapbender/activityindicator": "0.9.x",
        "mapbender/basesourceswitcher": "0.9.x",
        "mapbender/coordinatesdisplay": "0.9.x",
        "mapbender/copyright": "0.9.x",
        "mapbender/digitizer": "1.x",
        "mapbender/drupalintegration": "0.9.x",
        "mapbender/featureinfo": "0.9.x",
        "mapbender/gpsposition": "0.9.x",
        "mapbender/htmlelement": "0.9.x",
        "mapbender/imageexport": "0.9.x",
        "mapbender/kml": "0.9.x",
        "mapbender/layertree": "0.9.x",
        "mapbender/legend": "0.9.x",
        "mapbender/mobile": "0.9.x",
        "mapbender/ol3-map": "0.0.x",
        "mapbender/overview": "0.9.x",
        "mapbender/poi": "0.9.x",
        "mapbender/printclient": "0.9.x",
        "mapbender/ruler": "0.9.x",
        "mapbender/scalebar": "0.9.x",
        "mapbender/scaledisplay": "0.9.x",
        "mapbender/scaleselector": "0.9.x",
        "mapbender/searchrouter": "0.9.x",
        "mapbender/simplesearch": "0.9.x",
        "mapbender/sketch": "0.9.x",
        "mapbender/srsselector": "0.9.x",
        "mapbender/zoombar": "0.9.x",
        "mapbender/zoomcoordinate": "0.9.x"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/components",
        "component-baseurl": "/components"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "ATTENTION": "Symlinks creating under MS Windows works only if composer runs with admin permissions",
        "symfony-assets-install": "symlink"
    },
    "repositories": [
        { "type": "git", "url": "https://github.com/mapbender/mapbender-about.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-activityindicator.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-basesourceswitcher.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-coordinatesdisplay.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-copyright.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-digitizer.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-drupalIntegration.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-featureinfo.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-gpsposition.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-htmlelement.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-imageexport.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-kml.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-layertree.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-legend.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-mobile.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-map-ol3.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-overview.git" },
        { "type": "git", "url": "https://github.com/mapbender/mapbender-poi.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-printclient.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-ruler.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-scalebar.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-scaledisplay.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-scaleselector.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-searchrouter.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-simplesearch.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-sketch.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-srsselector.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-zoombar.git"},
        { "type": "git", "url": "https://github.com/mapbender/mapbender-zoomcoordinate.git"},
        {
            "type": "package",
            "package": {
                "name": "ohvitorino/openlayers3",
                "type": "component",
                "version": "3.5.0",
                "dist": {
                    "url": "http://github.com/ohvitorino/openlayers3/archive/3.5.0.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": ["ol.js"],
                        "styles": ["ol.css"]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/placeholdersjs",
                "type": "component",
                "version": "4.0.1",
                "dist": {
                    "url": "http://github.com/jamesallardice/Placeholders.js/archive/v4.0.1.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "dist/placeholders.min.js",
                            "dist/placeholders.jquery.min.js"
                        ]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/jquerydialogextendjs",
                "type": "component",
                "version": "2.0.4",
                "dist": {
                    "url": "http://github.com/ROMB/jquery-dialogextend/archive/2.0.4.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "scripts": ["build/*.min.js"]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "igosuki/compass-mixins",
                "type": "component",
                "version": "0.12.6",
                "dist": {
                    "url": "http://github.com/Igosuki/compass-mixins/archive/v0.12.6.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fontfacekit/open-sans",
                "type": "component",
                "version": "1.4.0",
                "dist": {
                    "url": "http://github.com/FontFaceKit/open-sans/archive/1.4.0.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "styles": [
                            "*.css"
                        ],
                        "files": [
                            "fonts/*/*"
                        ]
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "eslider/sasscb",
                "version":"dev-master",
                "source": {
                    "url": "https://github.com/eSlider/sassc-binaries.git",
                    "type": "git",
                    "reference":"develop"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/joii",
                "type": "component",
                "version": "3.1.1",
                "source": {
                    "url": "https://github.com/haroldiedema/joii.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": ["dist/joii.min.js"],
                        "files": ["dist/joii.js"]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
